{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb"
    ],
    "rules": {
        "react/jsx-props-no-spreading": ["error", {
            "html": "ignore"
        }],
        "react/jsx-no-bind": "off",
        "react/no-array-index-key": "off",
        "react/prop-types": "off",
        "no-param-reassign": "off"
    }
}
